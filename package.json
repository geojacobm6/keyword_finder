{
  "name": "keyword_finder",
  "version": "1.0.0",
  "description": "",
  "main": "js/main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . keyword-finder-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Keyword Finder\"",
    "package-linux": "electron-packager . keyword-finder-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/keyword_finder-darwin-x64/keyword_finder.app keyword-finder-app --out=release-builds --overwrite --icon=assets/icons/mac/icon.icns",
    "create-debian-installer": "electron-installer-debian --src release-builds/keyword-finder-app-linux-x64/ --arch amd64 --config debian.json"
  },
  "keywords": [],
  "author": "Geo Jacob",
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-installer-debian": "^1.0.1",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.0.0",
    "electron-winstaller": "^2.7.0"
  },
  "dependencies": {
    "get-port": "^4.0.0",
    "jquery": "^3.3.1",
    "python-shell": "^1.0.6"
  }
}
